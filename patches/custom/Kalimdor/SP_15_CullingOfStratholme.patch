+	void SetData(uint32 uiType, uint32 uiData)
+    {
+		bool needSave = false;
+        switch(uiType)
+        {
+            case TYPE_ARTHAS:
+			{
+                if (uiData == IN_PROGRESS)
+					needSave = true;
+
+				m_auiEncounter[0] = uiData;
+                break;
+			}
+			case TYPE_SALRAMM:
+			{
+                if (uiData == DONE)
+					needSave = true;
+
+                m_auiEncounter[1] = uiData;
+                break;
+			}
+            case TYPE_MEATHOOK:
+			{
+                if (uiData == DONE)
+					needSave = true;
+
+                m_auiEncounter[2] = uiData;
+                break;
+			}
+            case TYPE_EPOCH:
+			{
+                if (uiData == DONE)
+					needSave = true;
+
+                m_auiEncounter[3] = uiData;
+                break;
+			}
+            case TYPE_MALGANIS:
+			{
+				if (uiData == DONE) 
+				{
+                    DoRespawnGameObject(m_uiMalChestGUID,30*MINUTE); //respawn time for the chest 1 day
+					needSave = true;
+				}
+                if (uiData == IN_PROGRESS)
+                    DoUseDoorOrButton(m_uiMalGate2GUID);
+                m_auiEncounter[4] = uiData;
+                break;
+			}
+            case TYPE_INFINITE_CORRUPTOR:
+			{
+				if (uiData == DONE) 
+					needSave = true;
+                m_auiEncounter[5] = uiData;
+                break;
+			}
+		}
+        if (uiData == DONE || needSave)
+        {
+            OUT_SAVE_INST_DATA;
+
+            std::ostringstream saveStream;
+            saveStream << m_auiEncounter[0] << " " << m_auiEncounter[1] << " " << m_auiEncounter[2] << " " << m_auiEncounter[3] << " " << m_auiEncounter[4];
+
+            strInstData = saveStream.str();
+
+            SaveToDB();
+            OUT_SAVE_INST_DATA_COMPLETE;
+        }
+    }
