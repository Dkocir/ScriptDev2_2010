+struct MANGOS_DLL_DECL npc_keeper_remulosAI : public npc_escortAI
+{
+    npc_keeper_remulosAI(Creature* pCreature) : npc_escortAI(pCreature) { Reset(); }
+
+    bool m_bIsInFight;
+
+    uint32 m_uiHeal_Timer;
+    uint32 m_uiStarfire_Timer;
