+                if (m_uiEvent_Timer < uiDiff)
+                {
+                    if (pEranikus && pEranikus->GetHealth()*100 / pEranikus->GetMaxHealth() >= 20)
+                    {
+                        if (m_uiEvent_Count == 0)
+                        {
+                            for (uint8 i = 0; i < 5; ++i)
